//copy of array
/*
#include<stdio.h>
int main()
{
    int N;
    scanf("%d",&N);
    int arr[N],arr2[N];
    for(int i=0;i<N;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<N;i++)
    {
        arr2[i]=arr[i];
    }
    for(int i=0;i<N;i++)
    {
        printf("%d",arr2[i]);
