//Array sum
/*
#include<stdio.h>
int main()
{
    int arr[10],sum=0;
    for(int i=0;i<10;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("The elements in an array\n");
    for(int i=0;i<10;i++)
    {
        printf("%d",arr[i]);
        sum=sum+arr[i];
    }
    printf("\n");
    printf("%d",sum);
}
